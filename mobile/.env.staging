# Staging Environment Variables

# API Configuration
API_URL=https://staging-api.proxyagent.app
API_TIMEOUT=30000

# Feature Flags
ENABLE_ANALYTICS=true
ENABLE_CRASH_REPORTING=true
ENABLE_PERFORMANCE_MONITORING=true
ENABLE_DEBUG_TOOLS=true

# OAuth Configuration
GOOGLE_CLIENT_ID=your-staging-google-client-id.apps.googleusercontent.com
GOOGLE_OAUTH_REDIRECT_URI=https://staging.proxyagent.app/auth/callback

# Push Notifications
EAS_PROJECT_ID=your-eas-project-id

# Error Tracking
SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id
SENTRY_ENVIRONMENT=staging

# Analytics
AMPLITUDE_API_KEY=your-staging-amplitude-key
MIXPANEL_TOKEN=your-staging-mixpanel-token

# App Configuration
APP_ENV=staging
APP_VERSION=1.0.0-staging
