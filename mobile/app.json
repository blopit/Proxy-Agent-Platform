{
  "expo": {
    "name": "Proxy Agent Platform",
    "slug": "proxy-agent-platform",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.proxyagent.app",
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "proxyagent",
              "com.googleusercontent.apps.765534073366-98ffgpadh021rmhktv4l16lbnaih12t6"
            ]
          }
        ]
      }
    },
    "android": {
      "package": "com.proxyagent.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "proxyagent"
            },
            {
              "scheme": "com.googleusercontent.apps.765534073366-98ffgpadh021rmhktv4l16lbnaih12t6"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "proxyagent",
    "plugins": [
      "expo-router",
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.765534073366-98ffgpadh021rmhktv4l16lbnaih12t6"
        }
      ]
    ],
    "extra": {
      "storybookEnabled": true,
      "appScheme": "proxyagent",
      "apiBaseUrl": "http://localhost:8000/api/v1",
      "googleClientId": "${EXPO_PUBLIC_GOOGLE_CLIENT_ID}",
      "googleWebClientId": "${EXPO_PUBLIC_GOOGLE_WEB_CLIENT_ID}",
      "googleNativeClientId": "${EXPO_PUBLIC_GOOGLE_NATIVE_CLIENT_ID}",
      "githubClientId": "${EXPO_PUBLIC_GITHUB_CLIENT_ID}",
      "microsoftClientId": "${EXPO_PUBLIC_MICROSOFT_CLIENT_ID}",
      "microsoftTenant": "${EXPO_PUBLIC_MICROSOFT_TENANT}"
    }
  }
}
